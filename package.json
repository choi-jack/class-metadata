{
    "name": "class-metadata",
    "version": "0.1.2",
    "description": "Metadata reflection based on TypeScript's experimental support for stage 2 decorators and metadata.",
    "keywords": [
        "aop",
        "aspect-oriented-programming",
        "class",
        "decorator",
        "metadata",
        "reflect-metadata",
        "reflection"
    ],
    "files": [
        "./dist"
    ],
    "type": "module",
    "exports": "./dist/index.js",
    "engines": {
        "node": ">=20"
    },
    "scripts": {
        "lint": "eslint .",
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
        "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
        "prebuild": "npm run clean",
        "build": "tsc -p ./tsconfig.build.json",
        "clean": "rimraf --glob ./dist ./*.tgz ./coverage",
        "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
        "prepack": "npm run build"
    },
    "author": "choi-jack <choi.jaehyeok@outlook.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/choi-jack/class-metadata.git"
    },
    "dependencies": {
        "reflect-metadata": "^0.2.2"
    },
    "devDependencies": {
        "@choi-jack/eslint-config": "^0.1.5",
        "@eslint/js": "^9.8.0",
        "@jest/globals": "^29.7.0",
        "@stylistic/eslint-plugin": "^2.4.0",
        "@tsconfig/node20": "^20.1.4",
        "@types/eslint__js": "^8.42.3",
        "@types/node": "^20.14.13",
        "conventional-changelog-cli": "^5.0.0",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "ts-jest": "^29.2.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "typescript-eslint": "^7.17.0"
    }
}
